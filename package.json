{
  "name": "molybdenum",
  "version": "1.0.0",
  "description": "Minimal Authentication Server built on Node.js, Express.js, MySQL and Typescript. ",
  "scripts": {
    "start": "npx ts-node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc && npm run copy-static-assets",
    "test": "jest tests",
    "lint": "eslint --ext .js,.ts . --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "copy-static-assets": "ts-node copy-static-assets.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.1",
    "@types/formidable": "^2.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/nodemailer": "^6.4.4",
    "@types/otp-generator": "^4.0.0",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.38.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "email-validator": "^2.0.4",
    "eslint": "^8.0.1",
    "express": "^4.18.1",
    "formidable": "^2.0.1",
    "imagekit": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.5",
    "otp-generator": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.20.1",
    "shelljs": "^0.8.5",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.2",
    "@types/node": "^17.0.42",
    "@types/supertest": "^2.0.12",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/opeolluwa/molybdenum.git"
  },
  "keywords": [
    "node",
    "nodemailer",
    "typescript",
    "express",
    "mysql",
    "sequelize",
    "typeorm",
    "jest",
    "eslint"
  ],
